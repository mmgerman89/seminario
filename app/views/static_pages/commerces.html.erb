<div class="page-header">
  <h1>Comercios adheridos</h1>
</div>
<% if not @commerces.nil? %>
	<ul class="list-group">
		<li class="list-group-item list-group-item-info">
			<span class="badge">Sucursales</span>
			Nombre
		</li>
		<% @commerces.each do |com| %>
		  <li class="list-group-item">
		    <span class="badge"><%= com.branches.count %></span>
		    <%= com.name %>
		  </li>
		<% end %>
	</ul>
<% end %>

		